<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>Students</h2>
    <ol class="breadcrumb">
      <li>
        <%= link_to 'Home', administrator_root_path %>
      </li>
      <li>
        <%= link_to administrator_section_students_path(@section) do %>
          Manage Sections
        <% end %>
      </li>
      <li>
        <%= link_to "students", administrator_section_students_path(@section) %>
      </li>
      <li class="active">
        <strong>Students Bulk Insertion</strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2">
  </div>
</div>

<div class="wrapper wrapper-content  animated fadeInRight">
  <div class="row">
    <div class="col-sm-12">
      <div class="ibox">
        <div class="ibox-content">
          <h2>Students</h2>
          <p>
            All students enrolled in class <strong><%= @section.klass_name %></strong> section <strong><%= @section.name %></strong>.
          </p>

          <%= render 'bulk_form' %>
        </div>
      </div>
    </div>
  </div>
</div>

