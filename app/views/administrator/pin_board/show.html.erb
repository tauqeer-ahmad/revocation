<div class='row wrapper border-bottom white-bg page-heading'>
  <div class='col-lg-9'>
    <h2>
      <%= link_to :back, class: 'btn btn-sm btn-white m-t-n-xs' do %>
        <i class='fa fa-arrow-left'></i>
      <% end %>

      Pin Board
    </h2>

    <ol class='breadcrumb'>
      <li>
        <%= link_to 'Home', administrator_root_path %>
      </li>

      <li class='active'>
        <strong>Notes</strong>
      </li>
    </ol>
  </div>
</div>

<div class='wrapper wrapper-content animated fadeInRight'>
  <div class='row'>
    <div class='col-md-12'>
      <%= render 'note_create_form' %>
    </div>
  </div>

  <%= render 'notes', notes: current_user.notes %>
</div>
